classdef Agent
% This is the class that will hold the data for
% people participating in the experiment
    properties
        x % x position
        y % y position
        vx % x velocity
        vy % y velocity
    end
    
    methods
        function obj = initialPosition(x,y,x0,y0)
            %% This is the constructor for the agent
            obj.x = x;
            obj.y = y;
            obj.vx = (x-x0)/T;
        end
    end
end